class Ð¡alculator():
    
    def add(self, x, y):
        return x + y
    
    def subtract(self, x, y):
        return x - y
    
    def multiply(self, x, y):
        return x * y
    
    def divide(self, x, y):
        if y == 0:
            return "Error: division by zero"
        return x / y

    def exponentiation(self, x, y):
        if x == 0 and y<0:
            return "Error: 0 cannot be raised to a negative power"
        return x ** y
